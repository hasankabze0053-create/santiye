{
  "expo": {
    "name": "SantiyePro",
    "slug": "SantiyePro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "santiyepro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDevelopmentRegion": "tr"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash_8k_final.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulamanın fotoğraflarınıza erişmesi gerekiyor.",
          "cameraPermission": "Uygulamanın kameranıza erişmesi gerekiyor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}